{
    "name": "unicloud-code-snippet",
    "displayName": "Code Snippets for Unicloud",
    "description": "it's a code snippet plugin for Unicloud",
    "publisher": "unicloud-fed",
    "version": "1.0.4",
    "icon": "images/logo.png",
    "engines": {
        "vscode": "^1.36.0"
    },
    "script": {
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.vue.json"
            },
            {
                "language": "vue",
                "path": "./snippets/snippets.vue.json"
            },
            {
                "language": "vue-html",
                "path": "./snippets/snippets.vue.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.js.json"
            },
            {
                "language": "scss",
                "path": "./snippets/snippets.scss.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "git@10.0.53.138:frontend-labs/vscode-snippets.git"
    },
    "devDependencies": {
        "conventional-changelog-cli": "^2.0.23"
    }
}
